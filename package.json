        "@fortawesome/pro-regular-svg-icons": "^5.11.2",
        "@fortawesome/pro-solid-svg-icons": "^5.11.2",
        "@fortawesome/vue-fontawesome": "^0.1.8",
        "@observablehq/stdlib": "^3.2.1",
        "bootstrap": "^4.2.1",
        "core-js": "^3.4.0",
        "d3": "^5.12.0",
