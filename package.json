        "source-map-support": "^0.5.16",
        "vue": "^2.5.22",
        "vue-router": "^3.1.3",
        "vue-scrollto": "^2.17.1",
        "vuex": "^3.1.1",
        "xml-js": "^1.6.11"
    },
